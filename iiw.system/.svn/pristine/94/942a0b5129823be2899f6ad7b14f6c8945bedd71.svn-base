
.form-group .form-control.format-select {
    color: #000;
    padding: 0;
    height: 39px;
    border-radius: 0;
    background-color: rgba(255,255,255,0.5) !important;
}

.input-group.format-input-group .form-control {
    padding: 0;
    height: 39px;
    border-radius: 0;
}

.input-group.format-input-group .input-group-btn .btn {
    background-color: rgba(0,0,0,0.2);
    border: 1px solid rgba(255,255,255,0.5);
}

.input-group.format-input-group .input-group-btn .btn:hover {
    background-color: rgba(0,0,0,0.1);
}

.system-insidemapsetting-map {
    position: relative;
}

.insidemapsetting-mapbar {
    background-color: rgba(0,0,0,0.4);
}

.insidemapsetting-mapbar > .btn-group-justified {
    height: 100%;
    width: 70%;
    margin: auto;
}

.insidemapsetting-mapbar > .btn-group-justified > a {
    background-color: rgba(0,0,0,0);
}

.insidemapsetting-mapbar > .btn-group-justified > a:hover {
    background-color: rgba(0,0,0,0.15);
}

.insidemapsetting-mapbar > .btn-group-justified > a.select {
    background-color: rgba(0,0,0,0.3);
}

.insidemapsetting-mapbox {
    position: relative;
    background-color: rgba(0,0,0,0.3);
}

.insidemapsetting-mapbox-location {
    position: absolute;
    bottom: 5px;
    left: 5px;
    width: 100px;
    height: 30px;
    line-height: 30px;
    background-color: rgba(0,0,0,0.5);
    z-index: 2;
    text-align: center;
    border: 1px solid rgba(255,255,255,0.5);
}

.insidemapsetting-mapbox-fastmap {
    position: absolute;
    bottom: 5px;
    right: 5px;
    height: 30px;
    z-index: 2;
}

.insidemapsetting-mapbox-map {
    position: relative;
    overflow: hidden;
}

.insidemapsetting-mapbox-devicepage {
    position: absolute;
    top: 0;
    left: 0;
    background-color: #000;
    z-index: 3;
}

.insidemapsetting-mapbox-devicepage > div {
    background-color: rgba(255,255,255,0.2);
}

.insidemapsetting-mapbox-table tbody tr.select {
    background-color: rgba(223, 105, 26,0.6);
}

.insidemapsetting-mapbox-table tbody tr:hover,
.insidemapsetting-mapbox-table tbody tr.select:hover {
    background-color: rgba(223, 105, 26,0.8);
}

.insidemapsetting-mapbox-tag-box {
    position: absolute;
    top: 0;
    left: 0;
}

.insidemapsetting-mapbox-tag {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
}

.insidemapsetting-mapbox-coordinate-box {
    color: red;
}

.insidemapsetting-mapbox-coordinate-box .safe-map-tag .safe-map-tag-panel {
    box-shadow: 0 0 10px red inset;
}

.insidemapsetting-mapbox-info-container {
    background-color: rgba(0,0,0,0.3);
}

.insidemapsetting-mapbox-mask {
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.6);
    z-index: 2;
}

.insidemapsetting-mapbox-status {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
}

.insidemapsetting-mapbox-info {
    position: relative;
    padding: 5px;
}

.insidemapsetting-mapbox-info > div {
    position: relative;
}

.insidemapsetting-mapbox-info > div > div {
    width: 100%;
}

.insidemapsetting-mapbox-info-select.form-control {
    background: rgba(0,0,0,0.25);
    color: #fff;
    border-radius: 0;
    height: 39px;
}

.insidemapsetting-mapbox-info .table.with-bordered {
    border: 1px solid rgba(255,255,255,0.25);
    margin-top: 5px;
    background: rgba(0,0,0,0.25);
    color: #fff;
    font-size: 15px;
}

.insidemapsetting-mapbox-info .table.with-bordered td {
    border: 1px solid rgba(255,255,255,0.25);
}

.insidemapsetting-mapbox-tips {
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: rgba(0,0,0,0.3);
    color: #aaa;
    overflow: hidden;
}

.insidemapsetting-mapbox-btn {
    position: relative;
    overflow: hidden;
}

.insidemapsetting-mapbox-btn > .btn-group-justified {
    height: 70px;
    line-height: 70px;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple {
    position: relative;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div {
    margin: 5px 0 5px 10px;
    float: left;
    height: 70px;
    line-height: 70px;
    width: 160px;
    border-radius: 5px;
    background-color: rgba(0,0,0,0.5);
    text-align: center;
    letter-spacing: 2px;
    cursor: pointer;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:hover {
    background-color: rgba(0,0,0,0.3);
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 1) {
    background-color: rgba(51,102,51,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 1):hover {
    background-color: #336633;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 2) {
    background-color: rgba(204,153,102,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 2):hover {
    background-color: #CC9966;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 3) {
    background-color: rgba(153,51,51,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 3):hover {
    background-color: #993333;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 4) {
    background-color: rgba(51,51,102,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 4):hover {
    background-color: #333366;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 5) {
    background-color: rgba(51,102,102,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 5):hover {
    background-color: #336666;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 6) {
    background-color: rgba(153,102,0,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 6):hover {
    background-color: #996600;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 7) {
    background-color: rgba(102,153,153,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 7):hover {
    background-color: #669999;
}

.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 8) {
    background-color: rgba(51,51,102,0.7);
}
.insidemapsetting-mapbox-btn .mapbox-btn-multiple > div > div:nth-child(8n + 8):hover {
    background-color: #333366;
}

.insidemapsetting-mapbox-btn .mapbox-btn-single {
    display: table;
    padding: 0 50px;
}

.insidemapsetting-mapbox-btn .mapbox-btn-single > div {
    width: 200px;
    display: table-cell;
    vertical-align: middle;
}

.insidemapsetting-mapbox-btn .mapbox-btn-single > div > a {
    width: 100%;
    height: 40px;
    line-height: 40px;
    margin: 5px 0;
    padding: 0;
    letter-spacing: 2px;
}