                        <div class="iiw-system-repairFlow-container-body-content-manage" ng-if="repair.select == '1'">

                            <div class="col-lg-12 iiw-system-repairFlow-container-body-content-manage-rows">
                                <div class="col-lg-2 iiw-system-repairFlow-container-body-content-manage-rows-title">多个登记</div>
                                <div class="col-lg-7 iiw-system-repairFlow-container-body-content-manage-rows-write">
                                    <div class="col-lg-6 inputCheckBox">
                                        <input ng-style="{'width': '20px', 'height': '20px'}" type="checkbox">
                                        <div class="iiw-system-repairFlow-input-check" ng-click="repair.changeValue('value1', true)"><i ng-if="repair.data.value1" class="fa fa-check"></i></div>
                                        <div>开启</div>
                                    </div>
                                    <div class="col-lg-6 inputCheckBox">
                                        <input ng-style="{'width': '20px', 'height': '20px'}" type="checkbox">
                                        <div class="iiw-system-repairFlow-input-check" ng-click="repair.changeValue('value1', false)"><i ng-if="!repair.data.value1" class="fa fa-check"></i></div>
                                        <div>关闭</div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-12 iiw-system-repairFlow-container-body-content-manage-rows">
                                <div class="col-lg-2 iiw-system-repairFlow-container-body-content-manage-rows-title">设备截图</div>
