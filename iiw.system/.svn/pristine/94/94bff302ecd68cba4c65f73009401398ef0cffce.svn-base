<div class="iiw-system-repairFlow">
    <div ui-layout="{ flow: 'row'}" options="{dividerSize: '0'}">
        <div ui-layout-container size="65px">
            <h2 class="page-title">{{repair.title}}</h2>
        </div>
        <div class="iiw-system-repairFlow-container" ui-layout-container>
