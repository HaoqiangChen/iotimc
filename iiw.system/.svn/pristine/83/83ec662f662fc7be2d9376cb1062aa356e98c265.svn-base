.iiw-system-bigmonitorthemerelevance {
    position: relative;
    height: 100%;
}
.iiw-system-bigmonitorthemerelevance-container {
    background-color: rgba(0,0,0,0.3);
}
.iiw-system-bigmonitorthemerelevance-container-header {
    background-color: rgba(0,0,0,0.3);
    padding: 5px;
}
.iiw-system-bigmonitorthemerelevance-container-content {
    padding: 10px;
}
.iiw-system-bigmonitorthemerelevance-container-footer {
    background-color: rgba(0,0,0,0.3);
    padding: 5px;
    overflow: hidden;
}
.iiw-system-bigmonitorthemerelevanceitem-showTable-container {
    width:800px;
    height: 500px;
}
.iiw-system-bigmonitorthemerelevanceitem-showTable thead>tr {
    background-color: rgba(255,255,255,0.1);
}
.iiw-system-bigmonitorthemerelevanceitem-showTable tr:nth-child(2n) {
    background-color: rgba(255,255,255,0.1);
}
.iiw-system-bigmonitorthemerelevanceitem-showTable thead>tr,
.iiw-system-bigmonitorthemerelevanceitem-showTable tbody>tr {
    cursor: pointer;
    height: 40px;
}
.iiw-system-bigmonitorthemerelevanceitem-showTable thead>tr>th,
.iiw-system-bigmonitorthemerelevanceitem-showTable tbody>tr>td{
    padding: 10px;
    font-size: 14px;
}
.select {
    background-color: rgba(223, 105, 26,0.6);
}