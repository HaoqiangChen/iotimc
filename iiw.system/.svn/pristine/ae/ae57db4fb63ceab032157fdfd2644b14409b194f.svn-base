.iiw-system-expert-layout-container {
    border-top:1px solid rgba(255,255,255,0.3);
}

.iiw-system-form-container {
    border:1px solid rgba(255,255,255,0.3);
}

.iiw-system-page-container.ystyle .ui-layout-row{
    border: none;
}

.iiw-system-page-container.ystyle .ui-layout-row > .widget-content{
    border: none;
}


.iiw-system-page-container.ystyle .table td,
.iiw-system-page-container.ystyle .table th {
    vertical-align: middle;
    border-bottom: 1px solid rgba(0,0,0,0.3);
}

.iiw-system-page-container.ystyle .widget-content > .layout-full{
    overflow: auto;
}

.iiw-system-page-container.ystyle .widget-content > .layout-full > div {
    width: 100%;
}

.iiw-system-page-container.ystyle .table > tbody > tr > th:nth-child(1){
    padding: 10px 20px;
}
.iiw-system-page-container.ystyle .table > thead > tr > th:nth-child(1){
    width: 150px;
    position: relative;
}

.iiw-system-page-container.ystyle .table > thead > tr > th:nth-child(2){
    width: 200px;
}

.iiw-system-page-container.ystyle .table > thead > tr > th:nth-child(3){
    width: 300px;
}

.iiw-system-page-container.ystyle .table > thead > tr > th:nth-child(4){
    width: 100px;
}

.iiw-system-page-container.ystyle .ui-layout-row .layout-full.system-role-bar {
    background-color: rgba(0,0,0,.5);
    padding: 4px;
}

.iiw-system-page-container.ystyle .red input{
    border: 1px solid #d9534f;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > div{
    margin: 5px 0;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content{
    background-color: rgba(0,0,0,.3);
    padding: 10px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content > .col-md-12{
    height: 25px;
    line-height: 25px;
    padding: 0;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6{
    padding: 5px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .col-lg-12{
    width: 100%;
    background-color: rgba(0,0,0,.4);
    height: 45px;
    line-height: 45px;
    padding: 0;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table{
    overflow-y: scroll;
    background-color: rgba(0,0,0,.2);
    height: 500px;
    width: 100%;
    padding: 10px 0;
    box-shadow: inset 0 -5px 10px 1px rgba(0,0,0,.3);
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12{
    padding: 0;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12 i{
    line-height: 30px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12 i:hover{
    color: red;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12:hover{
    background-color: rgba(223, 105, 26,0.8);
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12:nth-child(odd):hover{
    background-color: rgba(223, 105, 26,0.8);
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12{
    cursor: pointer;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12:nth-child(odd){
    background-color: rgba(0,0,0,.2);
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table::-webkit-scrollbar{
    width: 6px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table::-webkit-scrollbar-thumb{
    background-color: rgba(200,200,200,.7);
    border-radius: 50px;
}

.iiw-system-page-container.ystyle .addStyle .widget-content .layout-full > .content .col-md-6 > .table > .col-lg-12.select{
    background-color: rgba(223, 105, 26,0.8);
}


.ystyle div::-webkit-scrollbar{
    width: 6px;
}

.ystyle div::-webkit-scrollbar-thumb{
    background-color: rgba(200,200,200,.7);
    border-radius: 50px;
}

.ystyle .table > tbody > tr > .text-center > button{
    height: 35px;
    line-height: 5px;
}

.ystyle .relateWindow{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.2);
    z-index: 2;
}
.ystyle .relateWindow > div{
    padding: 10px 20px;
    width: 1000px;
    height: 650px;
    margin: 7% auto;
    background-color: rgba(0,0,0,.9);
    border-radius: 20px;
    color: #eee;
}

.ystyle .relateWindow > div > h3{
    text-align: center;
    line-height: 65px;
    margin: 0;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2){
    border: solid 1px #333;
    padding: 0;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(1){
    background-color: rgb(0,0,0);
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(2){
    height: 480px;
    padding: 0;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(2) .col-lg-12:nth-child(odd){
    background-color: #222;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(1),
.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(2) .col-lg-12{
    height: 28px;
    line-height: 28px;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(2) .col-lg-12:hover{
    background-color: #df691a;
    cursor: pointer;
}

.ystyle .relateWindow > div > .col-lg-12:nth-child(2) > .col-lg-12:nth-child(2) .col-lg-12.select{
    background-color: #cd681a;
}

.ystyle .relateWindow button{
    margin: 15px 0 15px 15px;
}

.ystyle .relateWindow .input-group{
    position: absolute;
    right: 0;
    top: 15px;
}

.expander{
    border: 1px solid black;
    width: 250px;
}

.expander>.title{
    background-color: black;
    color: white;
    padding: .1em .3em;
    cursor: pointer;
}

.expander>.body{
    padding: .1em .3em;
}

