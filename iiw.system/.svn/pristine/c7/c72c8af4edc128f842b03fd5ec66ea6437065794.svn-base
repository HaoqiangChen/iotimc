.informationappear-header{
    overflow: hidden;
}
.informationappear-content-header{
    padding: 2px;
    background-color: rgba(0,0,0,0.3);
    border: 1px solid rgba(0,0,0,0.3);
    overflow:hidden;
}
.informationappear-main{
    padding: 15px;
    overflow:auto;
}
.informationappear-informationConfirm {
    height: 550px;
    overflow:auto;
}
.informationappear-content-select{
    height: 39px;
    width: 100px;
    background-color: rgba(0,0,0,0.3);
    color:#fff;
    border: 1px solid #666;
}
.informationappear-content-iconfirm-select{
    height: 37px;
    width: 300px;
    background-color: rgba(0,0,0,0.3);
    color:#fff;
    border: 1px solid #666;
}
.informationappear-content .errorMessage{
    border: 1px solid #ff4444;
}