<div class="iiw-system-page-container">
     <div class="stretch ui-layout-column" ui-layout="{ flow: 'column'}" options="{dividerSize:'0'}">
         <div class="ui-layout-row" ui-layout="{ flow: 'row'}" options="{dividerSize:'0'}">
             <div class="ui-layout-container iiw-system-hardwarerelate-related-header-container" size="300px" ui-layout-container>
                 <div class="pull-left iiw-system-hardwarerelate-main-related-container">
                     <div class="iiw-system-hardwarerelate-main-related-header">
                         <div class="layout-full iiw-system-hardwarerelate-main-related-header-title">
                             <i class="fa fa-fw fa-circle"></i>关联设备(<span>{{mainDevice.length}}</span>)<a class="pull-right" ng-click="cleanMainDevice()"><i class="fa fa-recycle"></i>清空</a>
                         </div>
                     </div>
                     <div class="iiw-system-hardwarerelate-main-related-device-content">
                         <div class="iiw-system-hardwarerelate-main-related-device-body" i-scroll>
                             <div>
                                 <a ng-repeat="device in mainDevice" title="点击移除" class="iiw-system-hardwarerelate-main-device-item text-center" ng-click="removeMainDevice(device)">
                                     <system-device-icon type="{{device.type}}"></system-device-icon>
                                     <span class="iiw-system-hardwarerelate-main-device-item-label">{{device.name}}</span>
                                 </a>
                             </div>
                         </div>
                     </div>
                 </div>
                 <div class="pull-left iiw-system-hardwarerelate-main-related-container">
                     <div class="iiw-system-hardwarerelate-main-related-header">
                         <div class="layout-full iiw-system-hardwarerelate-main-related-header-title">
                             <i class="fa fa-fw fa-circle"></i>联动设备(<span>{{relateDevice.length}}</span>)<a class="pull-right" ng-click="cleanRelateDevice()"><i class="fa fa-recycle"></i>清空</a>
                         </div>
                     </div>
                     <div class="iiw-system-hardwarerelate-main-related-device-content">
                         <div class="iiw-system-hardwarerelate-main-related-device-body" i-scroll>
                             <div ui-sortable="{cursor:'move'}" ng-model="relateDevice">
                                 <a ng-repeat="device in relateDevice" title="点击移除" class="iiw-system-hardwarerelate-main-device-item text-center" ng-click="removeRelateDevice(device)">
                                     <system-device-icon type="{{device.type}}"></system-device-icon>
                                     <span class="iiw-system-hardwarerelate-main-device-item-label">{{device.name}}</span>
                                 </a>
                             </div>
                         </div>
                     </div>
                 </div>
             </div>
             <div class="ui-layout-container iiw-system-hardwarerelate-device-layout-container" ui-layout-container>
                 <div class="pull-left iiw-system-hardwarerelate-main-related-container">
                     <div class="ui-layout-row" ui-layout="{ flow: 'row'}" options="{dividerSize:'0'}">
                         <div class="ui-layout-container iiw-system-hardwarerelate-main-related-device-filter-header" size="60px" ui-layout-container>
                             <div class="row layout-full" style="margin:0;">
                                 <div class="col-md-6 text-left iiw-system-hardwarerelate-main-related-col">
                                     <select ng-model="mainFilter.type" ng-change="filterMainDevice()" class="iiw-system-hardwarerelate-device-type-select-container">
                                         <option value="">所有</option>
                                         <option value="{{row.content}}" ng-repeat="row in deviceTypeList">{{row.name}}</option>
                                     </select>
                                 </div>
                                 <div class="col-md-6 text-right iiw-system-hardwarerelate-main-related-col">
                                     <input type="text" ng-model="mainFilter.searchText" placeholder="{{keyword}}" ng-change="searchDevice('main')"/>
                                 </div>
                             </div>
                         </div>
                         <div class="ui-layout-container iiw-system-hardwarerelate-main-related-device-table-container" ui-layout-container>
                             <div class="layout-full iiw-system-hardwarerelate-main-related-device-table-body" i-scroll>
                                 <div>
                                     <table class="table iiw-system-table-container">
                                         <thead>
                                             <tr>
                                                 <th class="iiw-system-table-header-container">
                                                     <input id="checkAllMain" type="checkbox" ng-model="mainFilter.selectMainAll" ng-change="selAllMainService()">
                                                 </th>
                                                 <th>设备名称</th>
                                                 <th>设备类型</th>
                                                 <th>设备IP</th>
                                                 <th>所属单位</th>
                                                 <th>是否关联</th>
                                             </tr>
                                         </thead>
                                         <tbody>
                                             <tr ng-repeat="row in mainDeviceList" ng-click="addDevice(row,'main')">
                                                 <td class="iiw-system-table-header-container">
                                                     <system-row-check ng-model="row.checked" class="rowCheck"></system-row-check>
                                                 </td>
                                                 <td class="hidden-xs">{{row.name}}</td>
                                                 <td class="hidden-xs">{{row.typename}}</td>
                                                 <td class="hidden-xs">{{row.ip}}</td>
                                                 <td class="hidden-xs">{{row.syouname}}</td>
                                                 <td class="">{{row.minordevice.length > 0?'已关联':'未关联'}}</td>
                                             </tr>
                                         </tbody>
                                     </table>
                                 </div>
                             </div>
                         </div>
                         <div class="pagination ui-layout-container pagination-layout-container" size="60px" ui-layout-container>
                             <uib-pagination boundary-links="true" first-text="&laquo;" last-text="&raquo;" class="pagination pull-right" total-items="mainTotalSize" ng-model="mainCurrentPage" ng-change="pageOnChanged('main')" items-per-page="pageSize" previous-text="上一页" next-text="下一页" max-size="5"></uib-pagination>
                         </div>
                     </div>
                 </div>
                 <div class="pull-left iiw-system-hardwarerelate-main-related-container">
                     <div class="ui-layout-row" ui-layout="{ flow: 'row'}" options="{dividerSize:'0'}">
                         <div class="ui-layout-container iiw-system-hardwarerelate-main-related-device-filter-header" size="60px" ui-layout-container>
                             <div class="row layout-full" style="margin:0;">
                                 <div class="col-md-6 text-left iiw-system-hardwarerelate-main-related-col">
                                     <select ng-model="relatedFilter.type" id="relateType" ng-change="filterRelateDevice(mainDeviceType)" class="iiw-system-hardwarerelate-device-type-select-container">
                                         <option value="">所有</option>
                                         <option value="{{row.content}}" ng-repeat="row in deviceTypeList">{{row.name}}</option>
                                     </select>
                                 </div>
                                 <div class="col-md-6 text-right iiw-system-hardwarerelate-main-related-col">
                                     <input type="text" ng-model="relatedFilter.searchText" placeholder="{{keyword}}" ng-change="searchDevice('relate')"/>
                                 </div>
                             </div>
                         </div>
                         <div class="ui-layout-container iiw-system-hardwarerelate-main-related-device-table-container" ui-layout-container>
                             <div class="layout-full iiw-system-hardwarerelate-main-related-device-table-body" i-scroll>
                                 <div>
                                     <table class="table iiw-system-table-container">
                                         <thead>
                                             <tr>
                                                 <th class="iiw-system-table-header-container">
                                                     <input id="checkAllRelate" type="checkbox" ng-model="relatedFilter.selectRelateAll" ng-change="selAllRelateService()">
                                                 </th>
                                                 <th>设备名称</th>
                                                 <th>设备类型</th>
                                                 <th>设备IP</th>
                                                 <th>所属单位</th>
                                             </tr>
                                         </thead>
                                         <tbody>
                                             <tr ng-repeat="row in relateDeviceList"  ng-click="addDevice(row,'relate')">
                                                 <td class="iiw-system-table-header-container">
                                                     <system-row-check ng-model="row.checked" class="rowCheck"></system-row-check>
                                                 </td>
                                                 <td class="hidden-xs">{{row.name}}</td>
                                                 <td class="hidden-xs">{{row.typename}}</td>
                                                 <td class="hidden-xs">{{row.ip}}</td>
                                                 <td class="hidden-xs">{{row.syouname}}</td>
                                             </tr>
                                         </tbody>
                                     </table>
                                 </div>
                             </div>
                         </div>
                         <div class="pagination ui-layout-container pagination-layout-container" size="60px" ui-layout-container>
                             <uib-pagination boundary-links="true" first-text="&laquo;" last-text="&raquo;" class="pagination pull-right" total-items="relateTotalSize" ng-model="relateCurrentPage" ng-change="pageOnChanged('relate')" items-per-page="pageSize" previous-text="上一页" next-text="下一页" max-size="5"></uib-pagination>
                         </div>
                     </div>
                 </div>
             </div>
             <div class="ui-layout-container iiw-system-hardwarerelate-button-container" size="60px" ui-layout-container>
                 <button class="btn btn-s-md btn-success" type="button" ng-click="save()" ng-disabled="mainDevice.length == 0">保     存</button>
                 <button class="btn btn-s-md btn-danger" type="button" ng-click="back()">返     回</button>
             </div>
         </div>
     </div>
 </div>