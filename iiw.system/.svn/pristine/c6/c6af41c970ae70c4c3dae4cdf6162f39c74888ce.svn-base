<div class="layout-full" style="background: rgba(0,0,0,0.3);">
    <div class="ui-layout-row" ui-layout="{ flow: 'row'}" options="{dividerSize:'0'}">
        <div class="ui-layout-container iiw-system-form-container" ui-layout-container>
            <div class="layout-full" i-scroll>
                <div>
                    <form class="form-horizontal label-left" id="entityItemForm" name="entityItemForm" novalidate>
                        <fieldset>
                            <legend class="section fw-bold">基础信息配置</legend>
                            <div class="control-group row">
                                <div class="col-md-2">
                                    <label class="control-label">所属单位：</label>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <input type="text" ng-model="dataimport.entityItem.syouname" ng-readonly="true"
                                               class="form-control" id="dataimport.entityItem.syoufk" required/>
                                    </div>
                                </div>
                                <div class="col-md-2"></div>
                            </div>
                            <div ng-repeat="item in dataimport.entityItem.templates">
                                <legend class="section fw-bold">{{item.name}}</legend>
                                <div class="control-group row" ng-repeat="val in item.values">
                                    <div class="col-md-2">
                                        <label class="control-label">{{val.name}}</label>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <input type="text" ng-model="val.value"
                                                   class="form-control" required ng-readonly="true"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div ng-repeat="item in dataimport.entityItem.details">
                                <legend class="section fw-bold">{{item.name}}</legend>
                                <div class="control-group row" style="padding: 0 15px;">
                                    <table class="table table-hover iiw-system-table-container">
                                        <thead>
                                        <tr>
                                            <th ng-repeat="row in item.itemname">{{row}}</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <tr ng-repeat="rows in item.itemlist track by $index">
                                            <td ng-repeat="row in rows track by $index">{{row}}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
        <div class="ui-layout-container iiw-system-form-button-layout-container" size="60px" ui-layout-container
             style="background: none;">
            <button class="btn btn-danger btn-s-md" ng-click="dataimport.cancel()">取消</button>
        </div>
    </div>
</div>
