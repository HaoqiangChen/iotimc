.iiw-system-bigmonitorchannel-container{
    position: relative;
    height: 100%;
}
.iiw-system-bigmonitorchannel-container .layout-full{
    width: 100%;
    height: 100%;
}
.iiw-system-bigmonitorchannel-container  .system-bigmonitorchannel-bar{
    background-color: rgba(0,0,0,0.3);
    padding: 4px;
}
.iiw-system-bigmonitorchannel-container  .system-bigmonitorchannel-bar .role-container-body{
    padding: 15px;
}
.iiw-system-bigmonitorchannel-container  .system-bigmonitorchannel-bar a{
    margin-right: 4px;
}
.iiw-system-bigmonitorchannel-container  .stretch{
    overflow: hidden;
}

.iiw-system-bigmonitorchannel-container  .addScroll{
    overflow: auto;
}

.iiw-system-bigmonitorchannel-container  .addScroll::-webkit-scrollbar{
    width: 6px;
}

.iiw-system-bigmonitorchannel-container  .addScroll::-webkit-scrollbar-thumb{
    background-color: rgba(200,200,200,.7);
    border-radius: 50px;
}

.iiw-system-bigmonitorchannel-container  .bigmonitorchannel-system-select{
    width: 350px;
    height: 100%;
    float: left;
    margin: 5px 40px;
}
.iiw-system-bigmonitorchannel-container  .bigmonitorchannel-index-system-select select{
    height: 40px;
}
.iiw-system-bigmonitorchannel-container  .bigmonitorchannel-select-option{
    background-color: rgba(0, 0, 0, 0.25);
    height: 32px;
}

.iiw-system-bigmonitorchannel-container  .iiw-confirm-box-prefix{
    margin: 5px 10px;
    width:55%;
}

.iiw-system-bigmonitorchannel-container  .iiw-confirm-box-select{
    background-color: rgba(0,0,0,0.25);
    border: 1px solid rgba(255,255,255,0.25);
    height: 32px;
}
.iiw-system-bigmonitorchannel-container  .bigmonitorchannel-container-foot{
    background-color: rgba(0, 0, 0, 0.3);
    border-top: 1px solid rgba(0, 0, 0, 0.15);
}

.iiw-system-bigmonitorchannel-container .stretch .promptInput{
    border: 1px red solid;
}

.iiw-system-bigmonitorchannel-container .bigmonitorchannel-container-body{
    padding: 0 15px 20px 15px;
}
.iiw-system-bigmonitorchannel-container .bigmonitorchannel-container-body .table th{
    vertical-align: middle;
}
.iiw-system-bigmonitorchannel-container .bigmonitorchannel-container-foot button{
    width:100px;
    height:39px;
    margin-top: 2px;
    margin-right: 3px;
}
