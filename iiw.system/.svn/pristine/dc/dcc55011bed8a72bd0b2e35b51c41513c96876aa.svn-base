.system-expert-page-content{
    background-color: rgba(0, 0, 0, 0.3);
}
.system-expert-container{
    padding: 15px;
}
.system-expert-header{
    background-color: rgba(0,0,0,0.3);
}
.system-expert-header input{
    width: 200px;
    height: 39px;
    margin-top: 3px;
    margin-right: 5px;
}
.expertSelect{
    background-color: rgba(0,0,0,0.25);
    border: 1px solid rgba(255,255,255,0.25);
    height: 32px;
}

.system-monitorpatrol-item {
    position: relative;
}

.system-monitorpatrol-item-container {
    background-color: rgba(0,0,0,0.3);
}

.iiw-system-tax-3dphoto select{
    background-color: rgba(0,0,0,.3);
    color: #fff;
    margin: 3px 25px 0 0;
    width: 186px;
    border: 1px solid rgba(255,255,255,.3);
}

.iiw-system-tax-3dphoto-type{
    width: 33.33%;
    height: 400px;
    float: left;
    padding: 15px 30px;
}

.iiw-system-tax-3dphoto-type .images{
    width: 100%;
    height: 85%;
    position: relative;
}

.iiw-system-tax-3dphoto-type .name{
    width: 100%;
    height: 15%;
    text-align: center;
    font-size: 22px;
}

.iiw-system-tax-3dphoto-type .image{
    float: left;
    padding: 10px;
    border-radius: 5px;
    background-color: #eee;
    border: 1px solid rgba(0,0,0,.1);
    position: absolute;
    left: 30%;
    top: 50%;
    cursor: pointer;
}

.iiw-system-tax-3dphoto-type .image img{
    max-width: 100%;
    max-height: 260px;
}

.iiw-system-tax-3dphoto-type .image > div{
    width: 180px;
    height: 180px;
    background-size: 100% 100%;
}

.iiw-system-tax-3dphoto-type .bgColor{
    width: 100%;
    height: 100%;
    border-radius: 10px;
    background-color: rgba(0,0,0,.5);
    text-align: center;
    line-height: 340px;
    font-size: 180px;
    color: #5d5;
    z-index: 2;
    position: relative;
    cursor: pointer;
    border: 1px solid rgba(0,0,0,.1);
}

.iiw-system-tax-3dphoto .ng-isolate-scope{
    width: 100%;
}