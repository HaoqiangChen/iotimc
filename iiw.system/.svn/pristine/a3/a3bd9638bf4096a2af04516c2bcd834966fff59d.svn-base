.iiw-system-expert-layout-container {
    border-top:1px solid rgba(255,255,255,0.3);
}

.iiw-system-form-container {
    border:1px solid rgba(255,255,255,0.3);
}

.iiw-system-reaction .ui-layout-row{
    border: none;
}

.iiw-system-reaction .ui-layout-row > .widget-content{
    border: none;
}


.iiw-system-reaction .table td,
.iiw-system-reaction .table th {
    vertical-align: middle;
    border-bottom: 1px solid rgba(0,0,0,0.3);
}

.iiw-system-reaction .ui-layout-row .layout-full.system-role-bar {
    background-color: rgba(0,0,0,.5);
    padding: 4px;
}

.iiw-system-reaction .red input{
    border: 1px solid #d9534f;
}





.iiw-system-reaction .add-reaction .addEvent{
    text-align: center;
    width: 35px;
    font-size: 35px;
    opacity: .6;
    cursor: pointer;
    transition: .3s;
}

.iiw-system-reaction .add-reaction .addEvent:hover{
    text-shadow: 0 0 8px white;
    opacity: 1;
}

.iiw-system-reaction .add-reaction .widget-content .layout-full > .col-lg-12 > .col-lg-12{
    padding: 20px;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .pull-left p{
    line-height: 35px;
}

.iiw-system-reaction .add-reaction .input-group{
    margin: 5px 0;
}

.iiw-system-reaction .add-reaction .input-group input{
    width: 180px;
    border: 1px solid #4e5d6c;
}

.iiw-system-reaction .add-reaction .input-group button{
    height: 35px;
    line-height: 5px;
    margin-left: -2px;
}

.iiw-system-reaction .add-reaction .widget-content{
    padding: 0;
}

.iiw-system-reaction .add-reaction .layout-full > .col-lg-12 > .col-lg-12 > .pull-left{
    width: 21%;
    height: 35px;
    margin: 5px 0;
}


.iiw-system-reaction .add-reaction .layout-full > .col-lg-12 > .proceed-event{
    padding: 20px 15px;
    position: relative;
    text-align: center;
}


.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12:nth-child(1){
    background-color: rgba(0,0,0,.6);
    height: 50px;
    line-height: 50px;
    padding: 0;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order{
    padding: 0;
    margin: 0;
    height: 75px;
    line-height: 35px;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order .col-lg-4{
    padding: 0;
    font-size: 20px;
}

.iiw-system-reaction .add-reaction .order .col-lg-4 i{
    font-size: 35px;
    color: #999;
}

.iiw-system-reaction .add-reaction .order .col-lg-4 .shift-action{
    color: #eee;
}

.iiw-system-reaction .add-reaction .order .col-lg-4 .shift-action:hover{
    text-shadow: 0 0 10px #eee;
    cursor: pointer;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order > div{
    padding: 20px;
    height: 75px;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order > div:first-child{
    padding: 20px 5px;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order > div:last-child{
    padding: 20px 0;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order:nth-child(odd){
    background-color: rgba(0,0,0,.2);
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order:nth-child(even){
    background-color: rgba(0,0,0,.4);
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order input,
.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .order select{
    margin: 0;
    width: 100%;
    height: 35px;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div{
    float: left;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(1){
    width: 10%;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(2){
    width: 20%;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(3){
    width: 20%;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(4){
    width: 20%;
    position: relative;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(5){
    width: 20%;
}

.iiw-system-reaction .add-reaction .col-lg-12 > .proceed-event > .col-lg-12 > div:nth-child(6){
    width: 10%;
}

.iiw-system-reaction .add-reaction .not-allowed{
    cursor: not-allowed;
}

.iiw-system-reaction .add-reaction .red > p{
    color: #d9534f;
}

.iiw-system-reaction .add-reaction  input{
    border: none;
    color: white;
}

.iiw-system-reaction .add-reaction .red > input,
.iiw-system-reaction .add-reaction .red > select{
    border: 1px #d9534f solid;
}

.iiw-system-reaction .add-reaction .red > input:focus,
.iiw-system-reaction .add-reaction .red > select:focus{
    outline: white;
}

.iiw-system-reaction .add-reaction .choiceDevice{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height:100%;
    background-color: rgba(255,255,255,.2);
    z-index: 10;
}

.iiw-system-reaction .add-reaction .choiceDevice .col-lg-12{
    padding: 0;
}

.iiw-system-reaction .add-reaction .choiceDevice > div{
    width: 1400px;
    height: 730px;
    background-color: rgba(0,0,0,.9);
    border-radius: 20px;
    margin: 5% auto;
    padding: 15px 25px;
    border: 1px solid #444;
    position: relative;
}

.iiw-system-reaction .add-reaction .choiceDevice > div .fa-close{
    position: absolute;
    right: 5px;
    top: 0;
    font-size: 40px;
    color: #ff4444;
}

.iiw-system-reaction .add-reaction .choiceDevice > div .fa-close:hover{
    color: #ff6666;
    cursor: pointer;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > p{
    text-align: center;
    line-height: 35px;
    font-size: 20px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-12:nth-child(2){
    height: 60px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3{
    height: 580px;
    padding: 0 20px 0 0;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:first-child{
    height: 50px;
    background-color: rgba(255,255,255,.4);
    line-height: 50px;
    text-align: center;
    font-size: 20px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child{
    height: 530px;
    padding: 0;
    border: 1px solid #777;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child > div{
    height: 470px;
    width: 100%;
    overflow-y: scroll;
    padding: 10px 20px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child > div > .col-lg-12{
    height: 35px;
    background-color: rgba(255,255,255,.3);
    border-radius: 5px;
    margin: 10px 0;
    line-height: 35px;
    padding: 0 10px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child > div > .col-lg-12 > i{
    position: absolute;
    right: -5px;
    top: -10px;
    font-size: 25px;
    color: #ff4444;
    cursor: pointer;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child > div > .col-lg-12 > i:hover{
    color: #ff6666;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-3 > .col-lg-12:last-child > .col-lg-12{
    position: absolute;
    right: 0;
    bottom: 0;
    padding: 5px;
    background-color: #333;
    height: 50px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9{
    border: 1px #777 solid;
    height: 580px;
    padding: 0;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(1){
    background-color: rgba(0,0,0,1);
    line-height: 40px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(2){
    height: 535px;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(2) > .col-lg-12{
    line-height: 50px;
    position: absolute;
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(2) > .col-lg-12 > .col-lg-12:nth-child(even){
    background-color: rgba(0,0,0,.9);
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(2) > .col-lg-12 > .col-lg-12:nth-child(odd){
    background-color: rgba(50,50,50,.9);
}

.iiw-system-reaction .add-reaction .choiceDevice > div > .col-lg-9 > .col-lg-12:nth-child(2) > .col-lg-12 > .col-lg-12:hover{
    background-color: rgba(223, 105, 26,0.8);
}

.iiw-system-reaction .add-reaction .choiceDevice select,
.iiw-system-reaction .add-reaction .choiceDevice input{
    background-color: rgba(255,255,255,.1);
}

.iiw-system-reaction .add-reaction .choiceDevice option{
    background-color: rgba(0,0,0,.3);
}

.iiw-system-reaction .add-reaction .choiceDevice .button{
    position: absolute;
    bottom: 0;
    right: 0;
    background-color: rgba(0,0,0,0);
    height: 55px;
    padding: 0 25px;
}

.iiw-system-reaction .add-reaction .choiceDevice input{
    border: 1px solid #666;
    width: 200px;
}

.iiw-system-reaction .add-reaction .choiceDevice button{
    margin: 0 0 0 10px;
}

.iiw-system-reaction .add-reaction .select{
    background-color: rgba(223, 105, 26,0.8)!important;
}

.iiw-system-reaction .add-reaction-action div,
.iiw-system-reaction .add-reaction-devices div{
    padding: 0;
}

.iiw-system-reaction .add-reaction-action > .col-lg-2{
    line-height: 35px;
}



.iiw-system-reaction .add-reaction-devices > .col-lg-8{
    height: 450px;
    background-color: rgba(0,0,0,.3);
}

.iiw-system-reaction .add-reaction-devices .content{
    padding: 5px;
    height: 400px;
    overflow-y: scroll;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3{
    height: 90px;
    padding: 5px;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3 > div{
    border-radius: 5px;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
    padding: 5px 0;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3 > div > .col-lg-12{
    height: 35px;
    line-height: 35px;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3 > div > .col-lg-12 i{
    margin: 0 10px;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3 > div > i{
    position: absolute;
    top: -5px;
    right: -5px;
    width: 25px;
    height: 25px;
    color: #ff4444;
    cursor: pointer;
    font-size: 25px;
}

.iiw-system-reaction .add-reaction-devices .content > .col-lg-3 > div > i:hover{
    color: #ff6666;
}

.iiw-system-reaction .add-reaction-devices .footer{
    position: absolute;
    right: 0;
    bottom: 0;
    height: 50px;
    padding: 5px 10px;
    background-color: rgba(0,0,0,.6);
}

.iiw-system-reaction .device-args > div{
    position: absolute;
    left: 10px;
    bottom: 53px;
    height: 10px;
    background-color: #dfdfdf;
    border: 1px solid rgba(0,0,0,.2);
    z-index: 3;
    border-radius: 5px;
    opacity: 0;
    display: none;
}

.iiw-system-reaction .device-args > div > div{
    height: 30px;
    color: #333;
    line-height: 30px;
}

.iiw-system-reaction .device-args > div > div:hover{
    background-color: #cfcfcf;
    cursor: pointer;
}

.iiw-system-reaction .color-red{
    color: #d9534f!important;
}

.iiw-system-reaction .border-red{
    border: 1px solid #d9534f!important;
}
