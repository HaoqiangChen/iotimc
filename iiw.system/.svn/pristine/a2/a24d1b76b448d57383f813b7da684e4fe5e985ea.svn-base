
.iiw-system-page-container.yStyle input{
    background-color: rgba(0,0,0,.3);
    height: 45px;
    color: #efefef;
}

.iiw-system-page-container.yStyle .ui-layout-container,
.iiw-system-page-container.yStyle .stretch .ui-layout-row{
    border: none;
}

 .iiw-system-page-container.yStyle button i{
     padding-right: 3px;
 }

.iiw-system-page-container.yStyle .iiw-system-menu-bar-container{
    padding: 0;
    margin: 0;
    width: 950px;
}

.iiw-system-page-container.yStyle .widget-header{
    padding: 3px;
}

.iiw-system-page-container.yStyle .widget-header .col-lg-2{
    padding: 0;
    height: 40px;
}

.iiw-system-page-container.yStyle .widget-header .col-lg-2 .col-md-3{
    padding: 0;
    margin: 0;
    height: 40px;
}

.iiw-system-page-container.yStyle .widget-header .col-lg-2 .col-md-3 p{
    margin: 0;
}

.iiw-system-page-container.yStyle .widget-header .col-lg-2 .col-md-9{
    padding: 0;
    margin: 4px 0;
}

.iiw-system-page-container.yStyle .col-lg-2 .col-md-9 select{
    height: 35px;
    background-color: rgba(0,0,0,.3);
}
.iiw-system-page-container.yStyle .widget-content{
    padding: 20px 0;
}

/*电视墙模式列表样式*/
.iiw-system-page-container.yStyle  .models{
    position: absolute;
    width: 100%;
    padding-bottom: 50px;
}

.iiw-system-page-container.yStyle  .table{
    position: relative;
    width: 100%;
    height: 500px;
    background-color: rgba(0,0,0,.3);
    padding: 2px;
}

.iiw-system-page-container.yStyle  .table button{
    position: absolute;
    bottom: -45px;
    right: 0;
    z-index: 2;
}

.iiw-system-page-container.yStyle .models  .table:hover{
    box-shadow: aqua 0 0 10px 1px;
    cursor: pointer;
}

.iiw-system-page-container.yStyle  .table > mask i{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 500px;
    font-size: 200px;
    background-color: rgba(0,0,0,.6);
}

.iiw-system-page-container.yStyle  .table .tr .td{
    height: 100%;
    float: left;
    padding: 2px 3px;
    position: relative;
}

.iiw-system-page-container.yStyle  .table > .tr > .td > div{
    background-color: rgba(0,0,0,.3);
    width: 100%;
    height: 100%;
    text-align: center;
    cursor: pointer;

}
.iiw-system-page-container.yStyle .addStyle  .table input{
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -20px;
    line-height: 40px;
    font-size: 25px;
}

/*添加电视墙模式样式*/
.iiw-system-page-container.yStyle .addStyle .widget-header{
    text-align: right;
}

.iiw-system-page-container.yStyle .addStyle button{
    width: 100px;
    height: 40px;
}

.iiw-system-page-container.yStyle .addStyle .widget-content{
    padding: 15px;
    overflow-y: auto;
}

.iiw-system-page-container.yStyle .addStyle .widget-content::-webkit-scrollbar{
    width: 6px;
}

.iiw-system-page-container.yStyle .addStyle .widget-content::-webkit-scrollbar-thumb{
    background-color: rgba(200,200,200,.7);
    border-radius: 50px;
 }

.iiw-system-page-container.yStyle .addStyle .widget-content > .row > input,
.iiw-system-page-container.yStyle .addStyle .widget-content > .row > p{
    float: left;
    height: 35px;
}

.iiw-system-page-container.yStyle .addStyle .widget-content > div > p{
    margin-top: 8px;
}

.iiw-system-page-container.yStyle .addStyle .row{
    height: 48px;
    width: 300px;
    float: left;
    padding: 0;
}

.iiw-system-page-container.yStyle .addStyle .row input{
    width: 180px;
}

.iiw-system-page-container.yStyle .addStyle .col-lg-12{
    height: 550px;
    margin: 0;
    padding-left: 0;
    padding-right: 20px;
}

.iiw-system-page-container.yStyle .addStyle .col-lg-12 .header{
    background-color: rgba(0,0,0,.5);
    height: 56px;
    padding: 8px 5px;
}

.iiw-system-page-container.yStyle .addStyle .col-lg-12 .content {
    background-color: rgba(0,0,0,.3);
    height: 485px;
    padding: 4px;
}

.iiw-system-page-container.yStyle .addStyle .footer{
    background-color: rgba(0, 0, 0, 0);
    height: 115px;
    padding: 0;
}

.iiw-system-page-container.yStyle .addStyle .footer p{
    height: 25px;
    margin: 0;
}

.iiw-system-page-container.yStyle .addStyle .header > h4,
.iiw-system-page-container.yStyle .addStyle .header > button,
.iiw-system-page-container.yStyle .addStyle .header > input{
    float: left;
    margin: auto 5px;
    line-height: 40px;
}

.iiw-system-page-container.yStyle .addStyle .header > input{
    width: 80px;
    height: 40px;
}

.iiw-system-page-container.yStyle .addStyle .header > button{
    padding-top: 0;
    margin-left: 50px;
}

.iiw-system-page-container.yStyle .addStyle .footer > textarea{
    min-width: 100%;
    max-width: 100%;
    min-height: 80px;
    max-height: 80px;
    color: #efefef;
    background-color: rgba(0,0,0,.3);
}

.iiw-system-page-container.yStyle .addStyle .header .btn-primary,
.iiw-system-page-container.yStyle .addStyle .header .btn-danger{
    float: right;
 }

.iiw-system-page-container.yStyle .red{
    color: #d9534f;
}

.iiw-system-page-container.yStyle .red input,
.iiw-system-page-container.yStyle .red textarea{
    border: 1px #d9534f solid;
}

.iiw-system-page-container.yStyle .remind{
    color: #d9534f;
}

.iiw-system-page-container.yStyle .remind2{
     color: #d44;
 }

/*测试窗口*/
.iiw-system-page-container.yStyle .test{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(255,255,255,.5);
}

.iiw-system-page-container.yStyle .test > div{
    width: 95%;
    height: 95%;
    background-color: rgba(0,0,0,.8);
    margin: auto;
    position: relative;
    top: 3%;
    border-radius: 20px;
    padding: 20px;
}
.iiw-system-page-container.yStyle .test > div > i{
    position: absolute;
    right: 25px;
    top: 15px;
    font-size: 30px;
}

.iiw-system-page-container.yStyle .test > div > i:hover{
    color: red;
    cursor: pointer;
}

.iiw-system-page-container.yStyle .test > div > h2{
    width: 50%;
    text-align: center;
    margin: auto;
}
.iiw-system-page-container.yStyle .test  .table{
    height: 850px;
    margin-top: 15px;
    margin-bottom: 10px;
    background-color: rgba(0,0,0,0);
    color: #bfbfbf;
}
.iiw-system-page-container.yStyle .test button{
    float: right;
    height: 5%;
    margin: 0 5px;
}
.iiw-system-page-container.yStyle .test .relevanceStyle{
    color: #3B983B;
}

.iiw-system-page-container.yStyle .test .relevanceStyle:hover{
    box-shadow: inset 0 0 15px 3px #3df;
    cursor: pointer;
}

.iiw-system-page-container.yStyle .test .notRelevanceStyle{
    color: #D83A39;
}

.iiw-system-page-container.yStyle .test .notRelevanceStyle:hover{
    cursor: not-allowed;
 }

.iiw-system-page-container.yStyle .test .modelFruit{
    overflow: hidden;
}

.iiw-system-page-container.yStyle .test p{
    font-size: 20px;
    margin-top: 5px;
    float: left;
}

.yStyle .testBigmonitor,
.yStyle .proceed{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
    z-index: 6;
    text-align: center;
    display: table;
}

.yStyle .testBigmonitor > i,
.yStyle .proceed > i{
    display: table-cell;
    vertical-align: middle;
    font-size: 45px;
}
