.iiw-system-tax-3dphoto-add .images{
    width: 100%;
    column-count: 4;
    column-gap: 10px;
}

.iiw-system-tax-3dphoto-add .image{
    background-color: #eee;
    padding: 10px;
    border: 1px solid rgba(0,0,0,.1);
    break-inside: avoid;
    position: relative;
    overflow: hidden;
}

.iiw-system-tax-3dphoto-add .image img{
    width: 100%;
}


.iiw-system-tax-3dphoto-add .image .name{
    width: calc(100% - 20px);
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 22px;
    position: absolute;
    left: 10px;
    bottom: 10px;
    background-color: rgba(0,0,0,.8);
    text-overflow: ellipsis;
    overflow: hidden;
}

/*.iiw-system-tax-3dphoto-add .image-look,*/
.iiw-system-tax-3dphoto-add .image-close{
    width: 80px;
    height: 80px;
    line-height: 80px;
    text-align: center;
    background-color: #000;
    color: #eee;
    border-radius: 50%;
    position: absolute;
    bottom: 60px;
    display: none;
    border: 1px solid rgba(0,0,0,.1);
    font-size: 30px;
    box-shadow: -2px 2px 5px 1px rgba(0,0,0,.3);
    cursor: pointer;
}

/*.iiw-system-tax-3dphoto-add .image:hover .image-look,*/
.iiw-system-tax-3dphoto-add .image:hover .image-close{
    display: block;
}

