.iiw-system-preplan-layout-container .panel-collapse {
    border-top:1px solid rgba(255,255,255,0.3);
}

.panel-heading textarea {
    background: transparent;
    width:100%;
    border:1px solid rgba(255,255,255,0.3);
    color:white;
}

.panel-body ul {
    list-style: none;
    padding:0;
}

.panel-body ul li {
    height:40px;
    line-height:40px;
}

.panel-body ul li:hover {
    background: rgba(255,46,46,0.5);;
}

.iiw-system-preplan-step-button-container {
    line-height: 51px;
    font-size: 20px;
}

.iiw-system-preplan-step-button-container .fa {
    padding: 0 10px;
    text-align: center;
}

.iiw-system-preplan-device-background-container {
    position: fixed;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:2;
    background: rgba(0,0,0,0.3);
}

.iiw-system-preplan-device-content-container {
    position: absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    width:1400px;
    height:700px;
    z-index:3;
    margin:auto;
    background: rgba(0,0,0,0.8);
    border-radius: 10px;
}

.iiw-system-preplan-device-list-container {
    width:50%;
    height:100%;
    float:left;
    position: relative;
    padding:0 10px 10px 0;
}

.iiw-system-preplan-device-list-container-inner {
    border:1px solid rgba(255,255,255,0.3);
    position: relative;
}

.iiw-system-preplan-device-content-body-container {
    padding:10px 0 10px 10px;
}

.iiw-system-preplan-device-list-inner {
    padding:10px;
}

.iiw-system-preplan-type-navbar {
    padding:10px;
}

.iiw-system-preplan-device-search-container {
    padding:0 10px;
}

.iiw-system-preplan-device-search-container input:focus {
    border:1px solid white;
}

.iiw-system-preplan-device-pagination-container {
    padding:10px 10px 0 0;
    margin:0;
}

.iiw-system-preplan-device-pagination-container .pagination {
    margin:0;
}

.iiw-system-preplan-device-table-container {
    padding:0 10px;
}

.iiw-system-preplan-device-table-list {
    height:100%;
    width:100%;
    position: relative;
    border-right:1px solid rgba(255,255,255,0.3);
    border-bottom:1px solid rgba(255,255,255,0.3);
    border-left:1px solid rgba(255,255,255,0.3);
}

.iiw-system-preplan-device-table-list .row {
    height:40px;
    line-height: 40px;
    margin:0;
    border-bottom:1px solid rgba(255,255,255,0.3);
}

.iiw-system-preplan-device-table-list .row:hover {
    background:#4e5d6c;
}

.row .iiw-system-preplan-device-attr-info {
    margin:0;
    padding:0;
}

.iiw-system-preplan-device-selected-header {
    line-height: 62px;
    text-align:center;
    border-bottom: 1px solid rgba(255,255,255,0.3);
    overflow: hidden;
}

.iiw-system-preplan-device-selected-list {
    position: relative;
}

.iiw-system-preplan-device-selected-list .row {
    height:40px;
    line-height: 40px;
    margin:0;
    border-bottom:1px solid rgba(255,255,255,0.3);
}

.iiw-system-preplan-device-selected-list .row:hover {
    background:#4e5d6c;
}

.iiw-system-preplan-device-choose-list-container {
    overflow:hidden;
}

.iiw-system-preplan-device-content-header-container {
    line-height: 60px;
    padding-left:10px;
}

.iiw-system-preplan-form-container {
    border:1px solid rgba(255,255,255,0.3);
    padding:10px;
    overflow: hidden;
}

.iiw-system-preplan-form-container > .layout-full {
    overflow-x: hidden;
    overflow-y: auto;
}

.iiw-system-preplan-wizard-btn {
    width:120px;
    border-radius: 10px;
}

.iiw-system-preplan-wizard-btn .fa {
    padding-right:10px;
}

.iiw-system-preplan-device-attr-info i {
    line-height: 40px;
    font-size: 20px;
    padding-right:10px;
    position: relative;
}

.iiw-system-preplan-device-attr-info i:hover {
    cursor:pointer;
    color:rgba(255,46,46,0.8);
}

.iiw-system-preplan-select-device-clean {
     padding-right:10px;
     color:white;
 }

.iiw-system-preplan-select-device-clean:hover {
    cursor:pointer;
    color:rgba(255,46,46,0.8);
}

.iiw-system-form-error {
    border:1px solid red!important;
}

.iiw-system-preplan-device-row-column {
    width:25%;
}

.iiw-system-preplan-device-row-column h5 {
    margin:0;
}

.iiw-system-preplan-device-row-column select {
    background:#777777;
    width:60%;
}

.iiw-system-preplan-step-runtime-container {
    line-height: 51px;
}

.iiw-system-preplan-step-runtime-container input {
    width: 100%;
    height: 35px;
}