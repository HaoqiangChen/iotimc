.iiw-system-timeline-add-time-icon {
    color:white;
}

.iiw-system-timeline-add-time-icon:hover {
    color:white;
}

.iiw-system-timeline-add-time-icon .fa {
    color:#5bc0de;
    padding-right:10px;
}

.iiw-system-rollcall-device-choose-container {
    position: relative;
}

.iiw-system-rollcall-syou-device-list-container {
    height:100%;
    width:50%;
    float:left;
    padding-right:5px;
}

.iiw-system-rollcall-syou-device-list-body {
    position: relative;
    background: rgba(0,0,0,0.3);
}

.iiw-system-rollcall-choose-device-list-container {
    height:100%;
    width:50%;
    float:left;
    padding-left:5px;
}

.iiw-system-rollcall-choose-device-list-body {
    position: relative;
    background: rgba(0,0,0,0.3);
}

.iiw-system-rollcall-syou-device-list-header {
    line-height: 40px;
    background: rgba(0,0,0,0.8);
    padding:0 10px;
}

.iiw-system-rollcall-syou-device-list-container .iiw-system-table-container tr,
.iiw-system-rollcall-choose-device-list-container .iiw-system-table-container tr {
    height:50px;
    line-height: 50px;
 }
 
 .iiw-system-table-container .iiw-system-rollcall-delete-title {
	text-decoration:none;
 }
 
 .iiw-system-table-container .iiw-system-rollcall-delete-title:hover {
	cursor:pointer;
	text-decoration:bottom;
	color:red;
 }